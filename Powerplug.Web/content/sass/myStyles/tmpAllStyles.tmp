// Custom green checkbox styles
// input.custom-checkbox-green {
    & + label {
        cursor: pointer;
        margin-bottom: 0;
    }
    &:checked {
        & + label {
            &:before {
                content: '';
                position: absolute;
                top: -10px;
                left: 0;
                width: 15px;
                height: 15px;
                border: 1px solid #d7d7d7;
            }
        }
    }
    &:not(checked) {
        position: absolute;
        opacity: 0;
        & + label {
            position: relative;
            padding: 0 0 0 30px;
            &:before {
                content: '\f00c';
                font-family: 'FontAwesome';
                font-size: 12px;
                color: #93c349;
                position: absolute;
                top: -10px;
                left: 0;
                width: 15px;
                height: 15px;
                border: 1px solid #989898;
                text-align: center;
            }
        }
    }
}
